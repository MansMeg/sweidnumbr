Depends: 
    R (>= 3.2)
Encoding: UTF-8
Imports: 
    lubridate (>= 1.5),
    stringr,
    checkmate
Suggests: 
    testthat (>= 1.0.0),
    rmarkdown,
    knitr
LazyData: true
RoxygenNote: 7.1.1
